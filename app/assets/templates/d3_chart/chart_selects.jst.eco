<div class="btn-group" style="float:right">
  <% for option, index in @chartSelects: %>
    <% if index is 0: %>
      <button class="btn btn-mini active" id="<%= option.nodeId %>"><%= option.label %></button>
    <% else: %>
      <button class="btn btn-mini" id="<%= option.nodeId %>"><%= option.label %></button>
    <% end %>
  <% end %>
</div>